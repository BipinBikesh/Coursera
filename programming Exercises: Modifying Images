//part 1

var img = new SimpleImage("smallhands.png");
print(img);
for (var pixel of img.values()){
    if(pixel.getX()<((1/3)*img.getWidth())){
        pixel.setRed(255);
    }
    if(pixel.getX()>((1/3)*img.getWidth())&&pixel.getX()<((2/3)*img.getWidth())){
        pixel.setGreen(255);
    }
    if(pixel.getX()>((2/3)*img.getWidth())){
        pixel.setBlue(255);
    }
}
print(img);
